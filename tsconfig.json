{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es2017", "esnext.asynciterable"],
		"sourceMap": true,
		"outDir": "./build",
		"rootDir": "./",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true
	}
}
